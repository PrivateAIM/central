{
    "name": "@privateaim/server-kit",
    "version": "0.4.0",
    "private": true,
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://tada5hi.net"
    },
    "license": "Apache-2.0",
    "description": "This package contains common constants, functions, types, ... for the server side.",
    "scripts": {
        "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "rimraf ./dist && cross-env NODE_ENV=production npm run build:js && npm run build:types"
    },
    "dependencies": {
        "@ebec/http": "^2.3.0",
        "@privateaim/core": "^0.4.0",
        "@privateaim/kit": "^0.4.0",
        "@socket.io/redis-emitter": "^5.1.0",
        "envix": "^1.5.0",
        "hapic": "^2.5.1",
        "singa": "^1.0.0",
        "winston": "^3.13.0"
    },
    "devDependencies": {
        "@authup/core-http-kit": "^1.0.0-beta.13",
        "@hapic/vault": "^2.3.3",
        "amqp-extension": "^3.3.0",
        "redis-extension": "^1.5.0",
        "typeorm-extension": "^3.5.1"
    },
    "peerDependencies": {
        "@authup/core-http-kit": "^1.0.0-beta.13",
        "@hapic/vault": "^2.3.3",
        "amqp-extension": "^3.3.0",
        "redis-extension": "^1.5.0",
        "typeorm-extension": "^3.5.1"
    },
    "publishConfig": {
        "access": "public"
    }
}
